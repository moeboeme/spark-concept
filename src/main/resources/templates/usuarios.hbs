{{#partial "contenido"}}
<div>
	<a href="/usuario"><button type="button" class="btn btn-primary">Nuevo</button></a>
</div>
<div class="table-responsive">
	<table class="table table-striped">
		<thead>
			<th>Nombre</th>
			<th>Apellido</th>
			<th>Nombre de usuario</th>
			<th></th>
		</thead>
		<tbody>
			{{#each usuarios}}
				<tr>
					<td>{{nombre}}</td>
					<td>{{apellido}}</td>
					<td>{{nombreDeUsuario}}</td>
					<td>
						<img src="/images/{{fotoDePerfil}}" style="width:50px;height:50px;">
					</td>
				</tr>
			{{/each}}
		</tbody>
	</table>
</div>
{{/partial}}
{{>pagina.hbs}}